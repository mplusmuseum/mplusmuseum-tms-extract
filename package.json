{
  "name": "alchemia",
  "version": "1.1.0",
  "main": "cli/index.js",
  "author": "Jonathan Dahan <hi@jonathan.is>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cli-progress": "^1.7.0",
    "cloudinary": "^1.10.0",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "compass": "^0.1.1",
    "del": "^3.0.0",
    "dotenv": "^5.0.0",
    "elasticsearch": "^13.3.1",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "get-stdin": "^5.0.1",
    "http-auth": "^3.2.3",
    "moment": "^2.20.1",
    "pretty-data": "^0.40.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "run-sequence": "^2.2.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "nodemon": "^1.14.10",
    "npm-run-all": "^4.1.2",
    "standard": "^10.0.3",
    "watch-run": "^1.2.5"
  },
  "bin": {
    "alchemia": "cli/index.js"
  },
  "scripts": {
    "start": "node --harmony cli/index.js",
    "tmsxml2json": "node --harmony cli/tmsxml2json.js",
    "json2elasticsearch": "node --harmony cli/json2elasticsearch.js",
    "test": "standard",
    "build": "npm-run-all build:prod build:prod:css",
    "build:prod": "babel src -d app",
    "build:prod:css": "compass compile",
    "build:dev": "npm-run-all -p build:dev:css build:dev:watch",
    "build:dev:css": "compass watch",
    "build:dev:watch": "watch-run -p 'src/**/*.js,server.js' 'npm run build'",
    "start:dev":
      "nodemon server.js -i src -i app/public/scripts -i app/data -i config.json",
    "go": "npm-run-all -p build:dev start:dev"
  }
}
